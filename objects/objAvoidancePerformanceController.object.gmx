          <argument>
            <kind>1</kind>
            <string>/// Timer
step = objZeaAvoidance.t;

switch (step)
{
    case 1:
        scrBGSettings(5, bgGridShape1A, 0, 0, 1, 1, 0, 0, 0, c_white, 0.3, 0);
    break;
    case 1619: 
        background_visible[0] = true;
    break;
    case 2401: 
        scrBGSettings(5, bgGridShape2, 0, 0, 1, 1, 0, 0, 1, c_white, 0.6, 0);
    break;
    
    // Circle colors
    case 3695:
    case 3741:
    
    case 8192: //Counter is reseted to 0 because this is the 2nd cycle
    case 8238:
        var hueRNG = irandom(255);
        var s = ic(0, 0, objBGCircleGradient);
        s . tag = counter;
        s . col1 = make_colour_hsv(hueRNG, 120, 250);
        s . col2 = make_colour_hsv(hueRNG, 180, 250);
        s . depth = 1000700 - counter;
        
        counter++;
        
        if !instance_exists(objBGLayer_Drawer)
        {
            var bg = ic(0, 0, objBGLayer_Drawer);
            //bg.BG = bgGridShape1B;
            
            bg.BG = background_index[5];
            bg.image_alpha = background_alpha[5];
            background_visible[5] = false;
        }
    break;
    
    case 3781: 
    case 8278: 
        with (objBGCircleGradient) 
            if tag == 0 instance_destroy(); 
    break;
    
    /*case 8277:
        instance_destroy(objBGLayer_Drawer);
    break;*/
    
    case 8374: 
        background_visible[5] = false;
        var grid = ic(0,0,objBGDraw);
        grid . bg = bgGridShape4; //star grid shape
        grid . depth = 1000054;
    break;
    
    case 9000:
        with objBGDraw { alarm[0] = 1; } //Fade out background
    break;
    case 9128:
        instance_destroy(objBGDraw);
        instance_destroy(objBGLayer_Drawer);
        scrBGSettings(5, bgGridShape1A, 0, 0, 1, 1, 0, 0, 1, c_white, 0.3, 0);
        var rngHUE = irandom(255);
        scrBGSettings(6, bgK3GridWideV, -16, 0, 1, 1, 0, -1, 1, make_colour_hsv(rngHUE, 113, 50), 0.6, 0);
        
        //background_color = make_colour_hsv(rngHUE,113,110);
        //scrBGSettings(6,bgK3BigGrid,16,0,1,1,0,-1,1,make_colour_hsv(rngHUE,113,50),0.2,0);
    break;
    
    // Rectangle grid size out
    case 4641:
        background_visible[5] = false;
        var hueRNG = 120;
        var s = ic(0,0,objBGCircleGradient);
        s . col1 = make_colour_hsv(hueRNG, 80, 202);
        s . col2 = make_colour_hsv(hueRNG, 162, 202);
        s . depth = 1000700;
        s . rad = 470;
        
        var bg = ic(0,0,objBGLayer_Drawer);
        bg.BG = bgGridShape1B;
        bg.image_alpha = 0.4;
        
        //scrBGSettings(6,bgK3GridSpike,0,0,1,1,0,-0.5,1,c_white,0.2,0);
    break;
    
    
    case 6062: 
        scrBGSettings(5, bgGridShape1B, 0, 0, 1, 1, 0, 0, 1, c_white, 0.4, 0);
    break;
    
    case 8191: counter = 0; break; // Reset the variable "counter"

}
</string>
